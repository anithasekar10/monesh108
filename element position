#include<stdio.h>
int main()
{
	int a,i,b;
	printf("\nEnter the size of the array:");
	scanf("%d",&a);
	printf("\nEnter the elements:");
	int arr[a];
	for(i=0;i<a;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("\nEnter the target element :");
	scanf("%d",&b);
	for(i=0;i<a;i++)
	{
		if(arr[i]==b)
		{
			printf("\n%d its index position is %d",b,i);
			break;
		}
	}
	return 0;
}
