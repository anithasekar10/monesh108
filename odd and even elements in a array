#include<stdio.h>
int main()	
{
	int a;
	printf("\nEnter the size of the array:");
	scanf("%d",&a);
	int arr[a];
	printf("\nEnter the array elements:\n");
	for(int i=0;i<a;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("\nThe even numbers in the array are:\n");
	for(int i=0;i<a;i++)
	{
		if(arr[i]%2==0)
			printf("%d  ",arr[i]);
	}
	printf("\nThe odd numbers in the array are:\n");
	for(int i=0;i<a;i++)
	{
		if(arr[i]%2!=0)
			printf("%d ",arr[i]);
	}
	return 0;
}
